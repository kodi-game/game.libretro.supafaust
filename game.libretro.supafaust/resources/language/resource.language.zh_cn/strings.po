msgctxt "Addon Summary"
msgid "Nintendo - SNES / SFC (Beetle Supafaust)"
msgstr "任天堂 - SNES / SFC (Beetle Supafaust)"

msgctxt "Addon Description"
msgid "Supafaust is a SNES emulator for multicore ARM Linux platforms. It is particularly suitable for low-end devices."
msgstr "Supafaust 是一个用于多核 ARM Linux 平台的 SNES 模拟器。它特别适用于低端设备。"

msgctxt "#30000"
msgid "Settings"
msgstr "设置"

msgctxt "#30001"
msgid "Pixel format"
msgstr "像素格式"

msgctxt "#30002"
msgid "Correct pixel aspect ratio"
msgstr "正确的像素纵横比"

msgctxt "#30003"
msgid "Horizontal blend/double filter"
msgstr "水平混合/双过滤器"

msgctxt "#30004"
msgid "Deinterlacer"
msgstr "逐行扫描"

msgctxt "#30005"
msgid "First displayed scanline in NTSC mode"
msgstr "NTSC 模式下第一条显示的扫描线"

msgctxt "#30006"
msgid "Last displayed scanline in NTSC mode"
msgstr "NTSC 模式下最后显示的扫描线"

msgctxt "#30007"
msgid "First displayed scanline in PAL mode"
msgstr "PAL 模式下第一条显示的扫描线"

msgctxt "#30008"
msgid "Last displayed scanline in PAL mode"
msgstr "PAL 模式下最后显示的扫描线"

msgctxt "#30009"
msgid "Region/Type of SNES to emulate"
msgstr "要仿真的 SNES 的区域/类型"

msgctxt "#30010"
msgid "Begin frame in SNES VBlank to lower latency"
msgstr "在 SNES VBlank 中开始帧以降低延迟"

msgctxt "#30011"
msgid "Enable 1-frame run-ahead"
msgstr "启用1帧预演"

msgctxt "#30012"
msgid "Enable multitap"
msgstr "启用多插转换器"

msgctxt "#30013"
msgid "CX4 clock rate %"
msgstr "CX4 时钟速率 %"

msgctxt "#30014"
msgid "SuperFX clock rate %"
msgstr "SuperFX 时钟速率 %"

msgctxt "#30015"
msgid "Emulate SuperFX instruction cache"
msgstr "模拟 SuperFX 指令缓存"

msgctxt "#30016"
msgid "Internal resampler(output rate)"
msgstr "内部重采样器（输出速率）"

msgctxt "#30017"
msgid "Emulation thread affinity mask"
msgstr "仿真线程关联掩码"

msgctxt "#30018"
msgid "PPU render thread affinity mask"
msgstr "PPU 渲染线程关联掩码"

msgctxt "#30019"
msgid "PPU renderer"
msgstr "PPU 渲染器"
