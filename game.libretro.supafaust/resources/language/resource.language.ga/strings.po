msgctxt "Addon Summary"
msgid "Nintendo - SNES / SFC (Beetle Supafaust)"
msgstr "Nintendo - SNES / SFC (Beetle Supafaust)"

msgctxt "Addon Description"
msgid "Supafaust is a SNES emulator for multicore ARM Linux platforms. It is particularly suitable for low-end devices."
msgstr "Is aithriseoir SNES é Supafaust do ardáin ilchroí ARM Linux. Tá sé oiriúnach go háirithe do ghléasanna íseal-deireadh."

# Empty string with ID 30000
msgctxt "#30001"
msgid "Pixel format"
msgstr "Formáid picteilín"

msgctxt "#30002"
msgid "Correct pixel aspect ratio"
msgstr "Cóimheas gné picteilín ceart"

msgctxt "#30003"
msgid "Horizontal blend/double filter"
msgstr "Cumasc cothrománach/scagaire dúbailte"

msgctxt "#30004"
msgid "Deinterlacer"
msgstr "Dí-idirfhíseáil"

msgctxt "#30005"
msgid "First displayed scanline in NTSC mode"
msgstr "An chéad líne scanadh a thaispeántar i mód NTSC"

msgctxt "#30006"
msgid "Last displayed scanline in NTSC mode"
msgstr "An líne scanadh dheireanach a taispeánadh i mód NTSC"

msgctxt "#30007"
msgid "First displayed scanline in PAL mode"
msgstr "An chéad líne scanadh a thaispeántar i mód PAL"

msgctxt "#30008"
msgid "Last displayed scanline in PAL mode"
msgstr "An líne scanadh dheireanach a taispeánadh i mód PAL"

msgctxt "#30009"
msgid "Region/Type of SNES to emulate"
msgstr "Réigiún/Cineál SNES le haithris"

msgctxt "#30010"
msgid "Begin frame in SNES VBlank to lower latency"
msgstr "Tosaigh an fráma i SNES VBlank chun an latency a ísliú"

msgctxt "#30011"
msgid "Enable 1-frame run-ahead"
msgstr "Cumasaigh rith 1-fhráma chun tosaigh"

msgctxt "#30012"
msgid "Enable multitap"
msgstr "Cumasaigh il-thapáil"

msgctxt "#30013"
msgid "CX4 clock rate %"
msgstr "Ráta cloig CX4 %"

msgctxt "#30014"
msgid "SuperFX clock rate %"
msgstr "Ráta cloig SuperFX %"

msgctxt "#30015"
msgid "Emulate SuperFX instruction cache"
msgstr "Aithris taisce treoracha SuperFX"

msgctxt "#30016"
msgid "Internal resampler(output rate)"
msgstr "Athshamplóir inmheánach (ráta aschuir)"

msgctxt "#30017"
msgid "Emulation thread affinity mask"
msgstr "Masc gaolmhaireachta snáithe aithrise"

msgctxt "#30018"
msgid "PPU render thread affinity mask"
msgstr "Masc cleamhnais rindreála PPU"

msgctxt "#30019"
msgid "PPU renderer"
msgstr "Rindreálaí PPU"
