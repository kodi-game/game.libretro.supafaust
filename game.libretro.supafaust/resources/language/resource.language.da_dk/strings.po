msgctxt "Addon Summary"
msgid "Nintendo - SNES / SFC (Beetle Supafaust) 1.24.0"
msgstr ""

msgctxt "Addon Description"
msgid "Supafaust is a SNES emulator for multicore ARM Linux platforms. It is particularly suitable for low-end devices."
msgstr "Supafaust er en SNES-emulator til multikerne ARM Linux-platforme. Den er især velegnet til enheder, der ikke er kraftige."

msgctxt "#30000"
msgid "Settings"
msgstr "Indstillinger"

msgctxt "#30001"
msgid "Pixel format"
msgstr "Pixelformat"

msgctxt "#30002"
msgid "Correct pixel aspect ratio"
msgstr "Korrekt billedformat for pixel"

msgctxt "#30003"
msgid "Horizontal blend/double filter"
msgstr "Vandret blandings-/dobbelt-filter"

msgctxt "#30004"
msgid "Deinterlacer"
msgstr "Deinterlacer"

msgctxt "#30005"
msgid "First displayed scanline in NTSC mode"
msgstr "Først viste scanlinje i NTSC-tilstand"

msgctxt "#30006"
msgid "Last displayed scanline in NTSC mode"
msgstr "Sidst viste scanlinje i NTSC-tilstand"

msgctxt "#30007"
msgid "First displayed scanline in PAL mode"
msgstr "Først viste scanlinje i PAL-tilstand"

msgctxt "#30008"
msgid "Last displayed scanline in PAL mode"
msgstr "Sidst viste scanlinje i PAL-tilstand"

msgctxt "#30009"
msgid "Region/Type of SNES to emulate"
msgstr "Region/type af SNES der emuleres"

msgctxt "#30010"
msgid "Begin frame in SNES VBlank to lower latency"
msgstr "Start-frame i SNES VBlank for at reducere latency"

msgctxt "#30011"
msgid "Enable 1-frame run-ahead"
msgstr "Aktiver 1-frame forudkørsel"

msgctxt "#30012"
msgid "Enable multitap"
msgstr "Aktiver multitap"

msgctxt "#30013"
msgid "CX4 clock rate %"
msgstr "CS4 clock-frekvens %"

msgctxt "#30014"
msgid "SuperFX clock rate %"
msgstr "SuperFX clock-frekvens %"

msgctxt "#30015"
msgid "Emulate SuperFX instruction cache"
msgstr "Emuler SuperFX-instruktionscache"

msgctxt "#30016"
msgid "Internal resampler(output rate)"
msgstr "Intern resampler (udgangsfrekvens)"

msgctxt "#30017"
msgid "Emulation thread affinity mask"
msgstr "Affinitetsmaske til emuleringstråden"

msgctxt "#30018"
msgid "PPU render thread affinity mask"
msgstr "Affinitetsmaske til PPU renderingstråd"

msgctxt "#30019"
msgid "PPU renderer"
msgstr "PPU-renderer"

