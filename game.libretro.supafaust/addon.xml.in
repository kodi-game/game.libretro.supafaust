<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.supafaust"
		name="Nintendo - SNES / SFC (Beetle Supafaust)"
		version="1.29.0.27"
		provider-name="Mednafen team">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>smc|swc|sfc|fig</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
		<license>GPLv2+</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.supafaust</source>
		<assets>
			<icon>resources/icon.png</icon>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<summary lang="da_DK">Nintendo - SNES / SFC (Beetle Supafaust)</summary>
		<summary lang="en_GB">Nintendo - SNES / SFC (Beetle Supafaust)</summary>
		<summary lang="es_ES">Nintendo - SNES / SFC (Beetle Supafaust)</summary>
		<summary lang="zh_CN">任天堂 - SNES / SFC (Beetle Supafaust)</summary>
		<description lang="da_DK">Supafaust er en SNES-emulator til multikerne ARM Linux-platforme. Den er især velegnet til enheder, der ikke er kraftige.</description>
		<description lang="de_DE">Supafaust ist ein SNES-Emulator für Mehrkern-ARM-Linux-Plattformen. Er ist besonders für Low-End-Geräte geeignet.</description>
		<description lang="en_GB">Supafaust is a SNES emulator for multicore ARM Linux platforms. It is particularly suitable for low-end devices.</description>
		<description lang="es_ES">Supafaust es un emulador de SNES para plataformas Linux ARM multinúcleo. Está especialmente indicado para dispositivos de gama baja.</description>
		<description lang="zh_CN">Supafaust 是一个用于多核 ARM Linux 平台的 SNES 模拟器。它特别适用于低端设备。</description>
	</extension>
</addon>
